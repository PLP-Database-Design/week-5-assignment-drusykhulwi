<table>
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Specialty</th>
        </tr>
    </thead>
    <tbody>
        <% providers.forEach(function(provider) { %>
            <tr>
                <td>
                    <%= provider.first_name %>
                </td>
                <td>
                    <%= provider.last_name %>
                </td>
                <td>
                    <%= provider.provider_specialty %>
                </td>
            </tr>
            <% }); %>
    </tbody>
</table>